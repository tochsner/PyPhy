{
  "model": "mymodel",
  "randomVariables": [
    {
      "dirichlet": {
        "distribution": {
          "type": "Dirichlet",
          "generates": "Dirichlet",
          "parameters": {
            "alpha": [
              1.0,
              1.0,
              1.0,
              1.0
            ]
          }
        }
      }
    },
    {
      "logNormal": {
        "distribution": {
          "type": "LogNormal",
          "generates": "LogNormal",
          "parameters": {
            "mean_log": 1.0,
            "sd_log": 0.5
          }
        }
      }
    },
    {
      "logNormal2": {
        "distribution": {
          "type": "LogNormal",
          "generates": "LogNormal",
          "parameters": {
            "mean_log": 0,
            "sd_log": 0.5
          }
        }
      }
    },
    {
      "logNormal3": {
        "distribution": {
          "type": "LogNormal",
          "generates": "LogNormal",
          "parameters": {
            "mean_log": 1,
            "sd_log": 0.5
          }
        }
      }
    },
    {
      "discreteGamma": {
        "distribution": {
          "type": "DiscreteGamma",
          "generates": "DiscreteGamma",
          "parameters": {
            "categories": 4,
            "shape": 0.5
          }
        }
      }
    },
    {
      "exponential": {
        "distribution": {
          "type": "Exponential",
          "generates": "Exponential",
          "parameters": {
            "rate": 10.0
          }
        }
      }
    },
    {
      "yule": {
        "distribution": {
          "type": "Yule",
          "generates": "Yule",
          "parameters": {
            "birth_rate": {
              "variable": "exponential"
            }
          }
        }
      }
    },
    {
      "phyloCTMC": {
        "distribution": {
          "type": "PhyloCTMC",
          "generates": "PhyloCTMC",
          "parameters": {
            "Q": {
              "variable": "hKY"
            },
            "site_rates": {
              "variable": "discreteGamma"
            },
            "tree": {
              "variable": "yule"
            }
          },
          "observedValue": [
            {
              "taxon": "human",
              "sequence": "ACGTACGTACGTACGTACGTACGT"
            },
            {
              "taxon": "chimp",
              "sequence": "ACGTACGTACGTACGTATGTACGT"
            },
            {
              "taxon": "gorilla",
              "sequence": "ACGTACGTACGCACGTACGTACG"
            }
          ]
        }
      }
    }
  ],
  "deterministicFunctions": [
    {
      "hKY": {
        "function": "hKY",
        "arguments": {
          "base_frequencies": {
            "variable": "dirichlet"
          },
          "kappa": {
            "variable": "logNormal + logNormal2 + logNormal3"
          }
        }
      }
    }
  ]
}